{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-visible-autofocus": {
      "projectType": "library",
      "root": "projects/ngx-visible-autofocus",
      "sourceRoot": "projects/ngx-visible-autofocus/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-visible-autofocus/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-visible-autofocus/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-visible-autofocus/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-visible-autofocus/src/test.ts",
            "tsConfig": "projects/ngx-visible-autofocus/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-visible-autofocus/karma.conf.js"
          }
        }
      }
    },
    "ngx-observable-directive": {
      "projectType": "library",
      "root": "projects/ngx-observable-directive",
      "sourceRoot": "projects/ngx-observable-directive/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-observable-directive/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-observable-directive/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-observable-directive/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-observable-directive/src/test.ts",
            "tsConfig": "projects/ngx-observable-directive/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-observable-directive/karma.conf.js"
          }
        }
      }
    },
    "test-env": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/test-env",
      "sourceRoot": "projects/test-env/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-env",
            "index": "projects/test-env/src/index.html",
            "main": "projects/test-env/src/main.ts",
            "polyfills": "projects/test-env/src/polyfills.ts",
            "tsConfig": "projects/test-env/tsconfig.app.json",
            "assets": [
              "projects/test-env/src/favicon.ico",
              "projects/test-env/src/assets"
            ],
            "styles": ["projects/test-env/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/test-env/src/environments/environment.ts",
                  "with": "projects/test-env/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test-env:build:production"
            },
            "development": {
              "browserTarget": "test-env:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-env:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/test-env/src/test.ts",
            "polyfills": "projects/test-env/src/polyfills.ts",
            "tsConfig": "projects/test-env/tsconfig.spec.json",
            "karmaConfig": "projects/test-env/karma.conf.js",
            "assets": [
              "projects/test-env/src/favicon.ico",
              "projects/test-env/src/assets"
            ],
            "styles": ["projects/test-env/src/styles.css"],
            "scripts": []
          }
        }
      }
    },
    "ngx-firebase-user-platform": {
      "projectType": "library",
      "root": "projects/ngx-firebase-user-platform",
      "sourceRoot": "projects/ngx-firebase-user-platform/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-firebase-user-platform/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-firebase-user-platform/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-firebase-user-platform/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-firebase-user-platform/src/test.ts",
            "tsConfig": "projects/ngx-firebase-user-platform/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-firebase-user-platform/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "9c9807ef-0ba4-4b76-b4dd-6f65d412f3fc"
  }
}
